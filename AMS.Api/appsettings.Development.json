{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "JwtSettings": {
    "SecretKey": "super-secret-key",
    "ExpireInHours": 24,
    "Issuer": "http://localhost:4200",
    "Audience": "http://localhost:5000"
  },
  "ConnectionStrings": {
    "New": "Server=localhost;Database=AmsDb;integrated security=True;MultipleActiveResultSets=true",
    "x": "Server=localhost;Database=AmsDb;Trusted_Connection=SSPI;Encrypt=false;TrustServerCertificate=true",
    "Default": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=AmsDb;Integrated Security=Tru"
  }
}
